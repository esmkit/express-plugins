{
  "name": "@esmkit/express-swagger",
  "version": "0.1.0",
  "description": "",
  "author": "Billgo <hi@billgo.me>",
  "bugs": "https://github.com/esmkit/express-plugins/issues",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": ["dist", "README.md"],
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./*": "./*"
  },
  "scripts": {
    "prepublishOnly": "bun run build",
    "build": "bunx rimraf dist && bun run build:esm && bun run build:bun",
    "build:bun": "bun run bun.build.mjs",
    "build:esm": "bunx tsup",
    "start": "bun run --watch examples/index.ts"
  },
  "dependencies": {
    "swagger-ui-dist": "^5.18.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@devjskit/biome-config": "^0.1.14",
    "@devjskit/copy-files": "^1.1.1",
    "@devjskit/kill-port": "^1.0.0",
    "@devjskit/rimraf": "^1.0.0",
    "@types/bun": "^1.1.14",
    "@types/node": "^22.10.1",
    "@types/express": "^5.0.0",
    "@types/serve-static": "^1.15.7",
    "@types/web": "^0.0.186",
    "express": "^4.21.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "express": "*"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}